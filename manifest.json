{
  "manifest_version": 3,
  "name": "Bitcoin Price Tracker",
  "version": "1.0",
  "description": "Displays the current Bitcoin price in Chrome.",
  "icons": {
    "128": "/images/bitcoin.png"
  },
  "permissions": ["alarms"],
  "background": {
    "service_worker": "sw.js"
  },
  "action": {
    "default_icon": {
      "128": "/images/bitcoin.png"
    }
  }
}
